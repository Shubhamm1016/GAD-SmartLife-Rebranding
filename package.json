{
  "name": "godrej.boyce",
  "version": "1.0.7",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15'",
    "run_simulator": "npx react-native run-ios --simulator= iPhone 15",
    "run_Mobile_Phone": "npx react-native run-ios --device iPhone ",
    "start": "npx react-native start --reset-cache",
    "prebuild": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "npm run clear && cd android  && ./gradlew assembleRelease && cd ../",
    "aab": "cd android && ./gradlew bundleRelease && cd ../",
    "test": "jest",
    "kil": "lsof -i tcp:8081 ,kill -9 PID",
    "buildIosClear": "rm -rf build",
    "delete": "rm -rf node_modules package-lock.json && npm install",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && rm -rf build && xcodebuild clean && cd ../",
    "removepodsFile": "cd ios && rm -rf Pods && rm -rf Podfile.lock && rm -rf build && pod cache clean --all && pod install",
    "Caseclean:IOS": "cd ios && pod cache clean --all && pod install",
    "protoFile": "protoc --proto_path=./ --js_out=import_style=commonjs,binary:./ provision.proto",
    "DerivedDataClear": "rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "CheckInstallationIos&Android": "npx react-native doctor",
    "AndroidFireBase": "cd android && ./gradlew signingReport",
    "lint": "eslint .",
    "build:ios": "npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle"
  },
